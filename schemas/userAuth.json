{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "User Auth",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "TimKels"
            ],
            "minLength": "6",
            "maxLength": "20"
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "passwords"
            ],
            "minLength": "6",
            "maxLength": "20"
        }
    },
    "examples": [{
        "username": "TimKels",
        "password": "passwords"
    }]
}