{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "User New",
    "required": [
        "username",
        "password",
        "firstName",
        "lastName"
    ],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "TIMMAY"
            ],
            "minLength": "6",
            "maxLength": "20"
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "Password"
            ],
            "minLength": "6",
            "maxLength": "20"
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "Tim"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "Kelley"
            ]
        },
        "isAdmin": {
            "type": "boolean",
            "default": false,
            "title": "The isAdmin Schema",
            "examples": [
                false
            ]
        }
    },
    "examples": [{
        "username": "TIMMAY",
        "password": "Password",
        "firstName": "Tim",
        "lastName": "Kelley",
        "isAdmin": false
    }]
}